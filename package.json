{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "credit": "echo Scripts obtained from https://github.com/silinternational/wecarry-ui",
    "prebuild:dev": "npm run clean:build",
    "build:dev": "rollup -c -w",
    "prebuild:prod": "npm run clean:build",
    "build:prod": "rollup -c",
    "clean": "run-p -l clean:build clean:deps",
    "clean:build": "rimraf dist/bundle* dist/service-worker* dist/workbox*",
    "clean:deps": "npm prune",
    "local:dev": "run-p -l build:dev serve:http",
    "local:prod": "run-p -l build:prod serve:http",
    "serve:http": "sirv public --dev --host 0.0.0.0 --port 5000 --single"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@roxi/routify": "^2.18.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "qs": "^6.10.1",
    "sirv-cli": "^1.0.0"
  }
}
